{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/employees/(.*)",
        "dest": "api/index.js"
      },
      {
        "src": "/api/employees",
        "dest": "api/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
}